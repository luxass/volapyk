{
  "name": "volapyk",
  "version": "1.5.0",
  "description": "Generate random and meaningless blocks or sequences",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@8.14.1",
  "license": "MIT",
  "homepage": "https://github.com/luxass/volapyk",
  "repository": "https://github.com/luxass/volapyk",
  "bugs": {
    "url": "https://github.com/luxass/volapyk/issues"
  },
  "keywords": [
    "volapyk",
    "volapyk-generator",
    "random words",
    "random texts"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "dev": "vitest",
    "test": "vitest run",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.0.0-beta.4",
    "@types/node": "^18.16.19",
    "eslint": "^8.54.0",
    "eslint-plugin-format": "^0.1.0",
    "tsup": "^8.1.0",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  }
}
