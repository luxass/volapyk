{
  "name": "volapyk",
  "version": "1.6.4",
  "description": "Generate random and meaningless blocks or sequences",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.5.2",
  "license": "MIT",
  "homepage": "https://github.com/luxass/volapyk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/volapyk.git"
  },
  "bugs": {
    "url": "https://github.com/luxass/volapyk/issues"
  },
  "keywords": [
    "volapyk",
    "volapyk-generator",
    "random words",
    "random texts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "vitest",
    "test": "vitest run",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@luxass/eslint-config": "^4.15.0",
    "@types/node": "^22.8.6",
    "eslint": "^9.19.0",
    "eslint-plugin-format": "^1.0.1",
    "publint": "^0.3.12",
    "tsdown": "^0.13.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
}
